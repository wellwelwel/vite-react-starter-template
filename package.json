{
   "name": "basic-vite-react-starter",
   "homepage": "dist",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "dev:eject": "rm -rf dist docker/backups",
      "dev:vite": "vite --host",
      "dev:stackblitz": "npm run init; npm run server:preview",
      "docker:backup": "mkdir -p docker/backups && tar --exclude='backups' --exclude='.DS_Store' --exclude='node_modules' -cvzf \"./docker/backups/$(date +'%y-%m-%d %T' | sed -e 's/:/-/g').tar.gz\" docker/.",
      "docker:compose": "npm run docker:eject &> /dev/null; npm run server:build && node scripts/node/mount.js --force && npm run docker:mount",
      "docker:eject": "cd docker && docker-compose --env-file ../.env down; rm -rf app",
      "docker:mount": "cd docker && docker-compose --env-file ../.env up -d",
      "docker:restart": "docker restart react-node-server",
      "docker:update": "node scripts/node/mount.js",
      "docker:update:build": "npm run server:build && node scripts/node/mount.js",
      "docker:watch": "node scripts/node/watch.js",
      "init": "npm i --ignore-scripts; npm run init:update",
      "init:clean": "npm run init; sh scripts/bash/clean.sh",
      "init:update": "npx ncu -u; npm i --ignore-scripts",
      "server:build": "vite build",
      "server:listen": "nodemon --delay 1 --watch server --watch .env --watch helpers server",
      "server:preview": "npm run server:build && npm run server:listen",
      "test:regex": "node tests/safe-regex"
   },
   "dependencies": {
      "@fvilers/disable-react-devtools": "^1.3.0",
      "axios": "^1.2.1",
      "body-parser": "^1.20.1",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-rate-limit": "^6.7.0",
      "express-session": "^1.17.3",
      "helmet": "^6.0.1",
      "html-entities": "^2.3.3",
      "memorystore": "^1.6.7",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.4.5"
   },
   "devDependencies": {
      "@types/react": "^18.0.26",
      "@types/react-dom": "^18.0.9",
      "@vitejs/plugin-legacy": "^2.3.1",
      "@vitejs/plugin-react": "^2.2.0",
      "autoprefixer": "^10.4.13",
      "cssnano": "^5.1.14",
      "node-watch": "^0.7.3",
      "nodemon": "^2.0.20",
      "npm-check-updates": "^16.5.1",
      "postcss": "^8.4.19",
      "prettier": "^2.8.1",
      "safe-regex": "^2.1.1",
      "sass": "^1.56.1",
      "terser": "^5.16.1",
      "vite": "^3.2.5"
   },
   "browserslist": [
      "> 0%"
   ],
   "imports": {
      "#helpers/*": "./helpers/*.js",
      "#server/*": "./server/*",
      "#server:configs/*": "./server/configs/*.js"
   }
}
