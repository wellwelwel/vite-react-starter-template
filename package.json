{
   "name": "basic-vite-react-starter",
   "homepage": "./dist/",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "build": "npm ci --ignore-scripts; npm audit fix; vite build; node ./express.js",
      "clean": "sh ./clean.sh;",
      "init": "npm i --ignore-scripts;",
      "postinit": "npm run update;",
      "start": "vite;",
      "update": "npx ncu -u; npm i --ignore-scripts; npm audit fix;"
   },
   "dependencies": {
      "@fvilers/disable-react-devtools": "^1.3.0",
      "axios": "^0.27.2",
      "express": "^4.18.1",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.4.1"
   },
   "devDependencies": {
      "@types/react": "^18.0.21",
      "@types/react-dom": "^18.0.6",
      "@vitejs/plugin-legacy": "^2.2.0",
      "@vitejs/plugin-react": "^2.1.0",
      "autoprefixer": "^10.4.12",
      "cssnano": "^5.1.13",
      "eslint": "^8.24.0",
      "npm-check-updates": "^16.3.2",
      "postcss": "^8.4.16",
      "prettier": "^2.7.1",
      "sass": "^1.55.0",
      "vite": "^3.1.3"
   },
   "browserslist": [
      "> 0%"
   ]
}
