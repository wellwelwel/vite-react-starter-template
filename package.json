{
   "name": "basic-vite-react-starter",
   "homepage": "dist",
   "private": true,
   "version": "0.1.0",
   "type": "module",
   "scripts": {
      "dev:eject": "rm -rf dist docker/backups",
      "dev:vite": "npx vite --host",
      "dev:stackblitz": "npm i --ignore-scripts; npm run server:preview",
      "docker:backup": "mkdir -p docker/backups && tar --exclude='backups' --exclude='.DS_Store' --exclude='node_modules' -cvzf \"./docker/backups/$(date +'%y-%m-%d %T' | sed -e 's/:/-/g').tar.gz\" docker/.",
      "docker:compose": "npm run docker:eject &> /dev/null; npm run server:build && node scripts/node/mount.js --force && npm run docker:mount",
      "docker:eject": "cd docker && docker-compose --env-file ../.env down; rm -rf app",
      "docker:mount": "cd docker && docker-compose --env-file ../.env up -d",
      "docker:restart": "docker restart react-node-server",
      "init": "npm i --ignore-scripts; npm run update",
      "init:clean": "npm run init; sh scripts/bash/clean.sh",
      "update": "npx npu; npm i --ignore-scripts",
      "preserver:build": "rm -rf './dist'",
      "server:build": "npx tsc && npx vite build",
      "server:listen": "npx nodemon -e ts --watch src/backend --watch .env --watch tsconfig.json --exec \"npx tsc --incremental && node dist/backend/server\"",
      "server:preview": "npm run server:build && npm run server:listen",
      "tests:regex": "node tests/safe-regex",
      "tests:hash": "node tests/create-hash"
   },
   "dependencies": {
      "@fvilers/disable-react-devtools": "^1.3.0",
      "axios": "^1.2.1",
      "body-parser": "^1.20.1",
      "compression": "^1.7.4",
      "connect-redis": "^6.1.3",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-rate-limit": "^6.7.0",
      "express-session": "^1.17.3",
      "helmet": "^6.0.1",
      "ioredis": "^5.2.4",
      "mysql2-orm": "^2.0.0",
      "node-and-vite-helpers": "^2.0.2",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.6.1"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.2",
      "@types/compression": "^1.7.2",
      "@types/connect-redis": "^0.0.19",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.15",
      "@types/express-session": "^1.17.5",
      "@types/node": "^18.11.17",
      "@types/react": "^18.0.26",
      "@types/react-dom": "^18.0.10",
      "@vitejs/plugin-legacy": "^3.0.1",
      "@vitejs/plugin-react": "^3.0.0",
      "autoprefixer": "^10.4.13",
      "cssnano": "^5.1.14",
      "node-watch": "^0.7.3",
      "nodemon": "^2.0.20",
      "packages-update": "^1.2.1",
      "postcss": "^8.4.20",
      "prettier": "^2.8.1",
      "safe-regex": "^2.1.1",
      "sass": "^1.57.1",
      "terser": "^5.16.1",
      "typescript": "^4.9.4",
      "vite": "^4.0.3"
   },
   "browserslist": [
      "> 0%"
   ]
}
